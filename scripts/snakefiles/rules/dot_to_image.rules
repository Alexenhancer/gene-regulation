rule dot_to_image:
    input: "{graph}.dot"
    output: "{graph}.{ext,(pdf|png)}"
    params: 
        qsub = config["qsub"] \
            + " -e {graph}_dot_to_{ext}_qsub.err" \
            + " -o {graph}_dot_to_{ext}_qsub.out"
    log: "{graph}_dot_to_{ext}.log"
    shell: """
dot -T{wildcards.ext} -o {output} {input} &> {log}
"""

